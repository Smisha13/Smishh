#include <stdio.h>


 int isEven(int number)
  {
      if(number%2==0)
      {
          return 2;
      }
  return 1;
  }
  int main()
  
  {
   int n;

   printf("Enter an integer : ");
   scanf("%d", &n);
   isEven(n);
   if(n%2==0)
   printf("the given number is even");
   else
   printf("the given number is odd");
}
